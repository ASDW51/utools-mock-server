{
    "main":"index.html",
    "logo":"logo.png",
    "features":[
        {
            "code":"mock-server",
            "explain":"本地创建mock服务",
            "cmds":[
                "mock",
                "mocks",
                "mock-server",
                "http",
                "mock",
                "mockjs"          
            ]
        },
        {
            "code":"setting",
            "explain":"设置mock-server数据,可以修正数据错乱导致的bug",
            "cmds":[
                "mock-setting",
                "设置"
            ]
        },
        {
            "code":"import",
            "explain":"输入ip加端口导入数据",
            "cmds":[
                {
                    "type":"regex",
                    "label":"mock-server",
                    "match":"/^(\\d{1,2}|1\\d\\d|2[0-4]\\d|25[0-5])\\.(\\d{1,2}|1\\d\\d|2[0-4]\\d|25[0-5])\\.(\\d{1,2}|1\\d\\d|2[0-4]\\d|25[0-5])\\.(\\d{1,2}|1\\d\\d|2[0-4]\\d|25[0-5])\\:?([0-9]|[1-9]\\d{1,3}|[1-5 ]\\d{4}|6[0-5]{2}[0-3][0-5])?$/",
                    "minLength":1
                }
            ]
        }
    ],
    "preload":"preload.js",
    "pluginSetting": {
        "single": false
    },
    "development":{
        "main":"http://127.0.0.1:3000/"
    }
}
